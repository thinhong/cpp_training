{
    "daysFollowUp": 10,
    "errorTolerance": 0.01,
    "timeStep": 1,
    "initialValues": {"S": 999, "I": 1, "R": 0, "V": 0},
    "parameters": {"beta": 1.2, "N": 1000},
    "transitions": {
        "S -> I": {"distribution": "mathExpression", "expression": "beta * S * I / N"}, 
        "S -> V": {"distribution": "frequency", "frequency": 2},
        "V -> R": {"distribution": "exponential", "rate": 1.5}, 
        "I -> R": {"distribution": "gamma", "scale": 2, "shape": 3}
    }
}